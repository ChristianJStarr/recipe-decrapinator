{
   "url":"www.allrecipes.com",
   "basic": {
      "title": {
         "type": "text-class",
         "container": "main-header",
         "text": "headline"
      },
      "image": {
         "type": "lazy",
         "container": "lead-content-aside-wrapper",
         "item": "lazy-image",
         "attr": "data-src"
      },
      "servings": {
         "type": "li-find",
         "container": "recipe-info-section",
         "item": "recipe-meta-item",
         "lookfor": ["Servings"],
         "exclude": ["Servings:"]
      },
      "yield": {
         "type": "li-find",
         "container": "recipe-info-section",
         "item": "recipe-meta-item",
         "lookfor": ["Yield"],
         "exclude": ["Yield:"]
      },
      "prep-time": {
         "type": "li-find",
         "container": "recipe-info-section",
         "item": "recipe-meta-item",
         "lookfor": ["Prep"],
         "exclude": ["Prep:"]
      },
      "total-time": {
         "type": "li-find",
         "container": "recipe-info-section",
         "item": "recipe-meta-item",
         "lookfor": ["Total"],
         "exclude": ["Total:"]
      }
   },
   "ingredients":{
      "type":"li",
      "container":"ingredients-section",
      "item":"ingredients-item",
      "text":"ingredients-item-name"
   },
   "directions":{
      "type":"li",
      "container":"instructions-section",
      "item":"instructions-section-item",
      "text":"paragraph"
   }
}